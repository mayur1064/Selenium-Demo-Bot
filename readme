#!/bin/bash

# Original string
original_string='This is the original string with the "substring" we want to "extract" between the two "markers".'

# Define the markers
start_marker='"substring"'
end_marker='"markers"'

# Extract substring between markers using sed with different delimiters
substring=$(echo "$original_string" | sed -n 's|.*'"$start_marker"'\(.*\)'"$end_marker"'.*|\1|p')

echo "Substring between markers: $substring"
