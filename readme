#!/bin/bash

# Set start date and today's date
START_DATE="2024-10-04"
END_DATE=$(date +%Y-%m-%d)  # This will give today's date

# Loop through dates from START_DATE to END_DATE
current_date="$START_DATE"
while [ "$current_date" != "$END_DATE" ]; do
  echo "$current_date"  # Output the current date in YYYY-MM-DD format
  current_date=$(date -I -d "$current_date + 1 day")  # Increment the date by 1 day
done

# Print the end date (optional)
echo "$END_DATE"
